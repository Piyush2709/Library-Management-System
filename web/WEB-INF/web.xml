<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>Add_books</servlet-name>
        <servlet-class>Add_books</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>update_books</servlet-name>
        <servlet-class>update_books</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>borrow</servlet-name>
        <servlet-class>borrow</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Login</servlet-name>
        <servlet-class>Login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>usersignup</servlet-name>
        <servlet-class>usersignup</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>accepted_borrow</servlet-name>
        <servlet-class>accepted_borrow</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>submit_feedback</servlet-name>
        <servlet-class>submit_feedback</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>returnbooks</servlet-name>
        <servlet-class>returnbooks</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Add_books</servlet-name>
        <url-pattern>/Add_books</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>update_books</servlet-name>
        <url-pattern>/update_books</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>borrow</servlet-name>
        <url-pattern>/borrow</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Login</servlet-name>
        <url-pattern>/Login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>usersignup</servlet-name>
        <url-pattern>/usersignup</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>accepted_borrow</servlet-name>
        <url-pattern>/accepted_borrow</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>submit_feedback</servlet-name>
        <url-pattern>/submit_feedback</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>returnbooks</servlet-name>
        <url-pattern>/returnbooks</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
